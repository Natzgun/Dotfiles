enable_audio_bell no
include ./Themes/Natzgun.conf
; include color.ini

font_family      CaskaydiaCove Nerd Font Mono
font_size 13

disable_ligatures never
# url_color #61afef

url_style curly

cursor_shape beam
cursor_beam_thickness 4

detect_urls yes

repaint_delay 10
input_delay 3
sync_to_monitor yes

background_opacity 0.9

; Change Shells zsh-bash-fish
shell fish


# BEGIN_KITTY_THEME
# moonlight
include current-theme.conf
# END_KITTY_THEME
